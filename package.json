{
  "name": "safe-formdata",
  "version": "0.1.0",
  "description": "Boundary-focused FormData parser with strict security guarantees",
  "author": "roottool",
  "repository": {
    "type": "git",
    "url": "https://github.com/roottool/safe-formdata"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "bun run type-check && bun run test && bun run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "24.10.4",
    "@vitest/coverage-v8": "4.0.16",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "license": "MIT",
  "keywords": [
    "formdata",
    "parser",
    "security",
    "boundary",
    "prototype-pollution"
  ]
}
